{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.py"
    }
  ],
  "functions": {
    "server.py": { 
      "maxDuration": 60,   
      "memory": 1024       
    }
  },
  "env": {
    "PYTHONUNBUFFERED": "1",
    "FLASK_ENV": "production",
    "FLASK_DEBUG": "0"
  }
} 